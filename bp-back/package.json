{
  "name": "",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "start:project": "node startup.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start": "nest start",
    "prepare": "husky",
    "pre-commit-husky": "npx lint-staged",
    "generate:module": "set \"MODULE_NAME=%npm_config_module%\" && python ./module-generator/generate-module.py -m %npm_config_module% && npm run lint",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:start:deploy": "prisma migrate deploy && prisma db seed && node dist/src/main.js",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:up": "docker compose -p test_api-aginco -f docker-compose-test.yml up -d",
    "db:test:down": "npm run stop:docker:dev && docker-compose -p test_api-aginco -f docker-compose-test.yml down",
    "db:test:restart": "npm run db:test:down && npm run db:test:up && node ./wait-for.ts && npm run prisma:test:deploy",
    "db:dev:up": "docker compose -p dev_api-aginco -f docker-compose.yml up -d",
    "db:dev:down": "docker-compose -p dev_api-aginco -f docker-compose.yml down",
    "db:dev:restart": "npm run db:dev:down && npm run db:dev:up && npm run prisma:dev:deploy",
    "stop:docker:dev": "node ./docker.stop.ts -- dev",
    "stop:docker:test": "node ./docker.stop.ts -- test",
    "test:all": "dotenv -e .env.test -- jest --no-cache --config ./jest.all.config.json && npm run stop:docker:test",
    "pretest:all": "npm run stop:docker:dev && npm run db:test:restart && dotenv -e .env.test npx prisma db seed",
    "test": "jest --no-cache --config ./jest.config.json",
    "test:watch": "jest --watch --config ./jest.config.json",
    "test:cov": "jest --coverage",
    "test:e2e": "dotenv -e .env.test -- jest --no-cache --config ./jest-e2e.json && npm run && npm run stop:docker:test",
    "pretest:e2e": "npm run stop:docker:dev && npm run db:test:restart && dotenv -e .env.test npx prisma db seed",
    "test:e2e:dev": "dotenv -e .env.test -- jest --watch --no-cache --config ./jest-e2e.json",
    "pretest:e2e:dev": "npm run stop:docker:dev && npm run db:test:restart && dotenv -e .env.test npx prisma db seed",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/testing": "^9.4.3",
    "@nestjs/throttler": "^5.0.0",
    "@prisma/client": "^5.4.2",
    "@sendgrid/mail": "^8.1.0",
    "@types/qrcode": "^1.5.2",
    "aws-sdk": "^2.1473.0",
    "bcrypt": "^5.1.1",
    "brazilian-class-validator": "^0.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cpf": "^2.0.1",
    "date-fns": "^3.3.1",
    "eslint": "^8.51.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "exceljs": "^4.4.0",
    "generate-password-ts": "^1.6.5",
    "helmet": "^7.0.0",
    "inquirer": "^9.2.12",
    "npm": "^10.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "prisma-pagination": "^0.2.3",
    "puppeteer": "^21.3.8",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "27.5.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "28.0.3",
    "lint-staged": "^15.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}